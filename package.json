{\n  "name": "error-screamer",\n  "displayName": "Error Screamer - FAAAH!",\n  "description": "Screams FAAAH when errors appear in the terminal",\n  "version": "0.0.1",\n  "engines": {\n    "vscode": "^1.85.0"\n  },\n  "categories": [\n    "Other"\n  ],\n  "activationEvents": [\n    "onStartupFinished"\n  ],\n  "main": "./out/extension.js",\n  "contributes": {\n    "commands": [\n      {\n        "command": "error-screamer.toggleScreaming",\n        "title": "Toggle Error Screaming"\n      }\n    ]\n  },\n  "scripts": {\n    "vscode:prepublish": "npm run compile",\n    "compile": "tsc -p ./",\n    "watch": "tsc -watch -p ./",\n    "pretest": "npm run compile && npm run lint",\n    "lint": "eslint src --ext ts"\n  },\n  "devDependencies": {\n    "@types/vscode": "^1.85.0",\n    "@types/node": "^20.0.0",\n    "typescript": "^5.0.0"\n  }\n}